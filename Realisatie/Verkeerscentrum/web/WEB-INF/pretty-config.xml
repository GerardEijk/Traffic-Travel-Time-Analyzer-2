<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="index">
		<pattern value="/" />
		<view-id value="/faces/index.xhtml" />
	</url-mapping>
        
        <url-mapping id="resources-img">
		<pattern value="/resources/img" />
                <view-id value="/resources/img" />
	</url-mapping>
        
        <url-mapping id="resources-css">
		<pattern value="/resources/css" />
                <view-id value="/resources/css" />
	</url-mapping>
        
        <url-mapping id="resources-js">
		<pattern value="/resources/js" />
                <view-id value="/resources/js" />
	</url-mapping>
        
       
        <url-mapping id="live">
		<pattern value="/live" />
		<view-id value="/faces/live.xhtml" />
	</url-mapping>
        
        <url-mapping id="analyse">
		<pattern value="/analyse" />
		<view-id value="/faces/analyse.xhtml" />
	</url-mapping>
               
        <url-mapping id="analyse-datasources">
		<pattern value="/analyse/datasources" />
		<view-id value="/faces/pages/analyse/datasourcecompare.xhtml" />
	</url-mapping>
        
        <url-mapping id="analyse-avgtraffic">
		<pattern value="/analyse/avgtraffic" />
		<view-id value="/faces/pages/analyse/avgtraffic.xhtml" />
	</url-mapping>
        
        <url-mapping id="analyse-weekdays">
		<pattern value="/analyse/weekdays" />
		<view-id value="/faces/pages/analyse/weekdaycompare.xhtml" />
	</url-mapping>
        
        <url-mapping id="analyse-rushhours">
		<pattern value="/analyse/rushhours" />
		<view-id value="/faces/pages/analyse/rushhours.xhtml" />
	</url-mapping>
        
        <url-mapping id="analyse-periods">
		<pattern value="/analyse/periods" />
		<view-id value="/faces/pages/analyse/periodcompare.xhtml" />
	</url-mapping>
        
        <url-mapping id="settings-routes">
		<pattern value="/settings/routes" />
		<view-id value="/faces/pages/settings/routes.xhtml" />
	</url-mapping>
        
        <url-mapping id="settings-routes-new">
		<pattern value="/settings/routes/new" />
		<view-id value="/faces/pages/settings/routes-new.xhtml" />
	</url-mapping>
        
        <url-mapping id="settings-routes-edit">
		<pattern value="/settings/routes/edit/#{id : routeSettings.id}" />
		<view-id value="/faces/pages/settings/routes-edit.xhtml" />
	</url-mapping>
        
        <!--  -->
        
        <url-mapping id="settings-routes-del">
		<pattern value="/settings/routes/del/#{id : routeSettings.id}" />
		<view-id value="/faces/pages/settings/routes-del.xhtml" />
	</url-mapping>
        
        <url-mapping id="settings-routes-details">
		<pattern value="/settings/routes/detail/#{id : routeSettings.id}" />
		<view-id value="/faces/pages/settings/routes-details.xhtml" />
	</url-mapping>
        
       
       
                
        <url-mapping id="settings">
		<pattern value="/settings" />
		<view-id value="/faces/settings.xhtml" />
	</url-mapping>
        
        <url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/faces/login.xhtml" />
	</url-mapping>
        

</pretty-config>